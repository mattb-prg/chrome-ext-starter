{
  "manifest_version": 2,
  "name": "Chrome Extension Starter",
  "description": "A template for chrome extensions",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "permissions": [
  ],
   "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/content.js"]
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  }
}